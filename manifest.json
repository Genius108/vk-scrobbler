{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "__MSG_extName__",
  "description": "__MSG_extDescr__",
  "icons": {
    "128": "img/icon128.png"
  },
  "version": "1.0.2",
  "manifest_version": 2,
  "minimum_chrome_version": "35.0",
  "background": {
    "page": "blocks/background/background.html"
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://*/*",
    "https://*/*",
    "cookies"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://vk.com/*",
        "https://vk.com/*"
      ],
      "js": [
        "blocks/indicators/indicators__utils.js",
        "blocks/indicators/indicators.js",
        "blocks/content/content__messages.js",
        "blocks/content/content__bus.js",
        "blocks/content/content__vk-patcher.js",
        "blocks/content/content__utils.js",
        "blocks/content/content.js"
      ],
      "css": [
        "blocks/indicators/indicators.css"
      ]
    }
  ],
  "default_locale": "ru",
  "homepage_url": "http://vkontakte.ru/vk_scrobbler",
  "web_accessible_resources": [
    "img/icon_eq.gif",
    "img/check.png",
    "img/icon_eqB.gif",
    "img/checkB.png",
    "img/heartB.png",
    "img/icon_eq_pause.png",
    "img/heartBW.png",
    "img/twitter.png",
    "img/icon_eq_pw.png",
    "img/pause.png",
    "img/save.gif",
    "blocks/vk-inner/vk-inner.js",
    "html/2.png"
  ],
  "content_security_policy": "script-src 'self' https://ws.audioscrobbler.com https://ssl.google-analytics.com; object-src 'self'; style-src 'self' 'unsafe-inline';"
}
